{
    "openapi": "3.0.3",
    "info": {
        "title": "API Gensmart APP",
        "version": "1.0.0"
    },
    "paths": {
        "/v1/user/register": {
            "post": {
                "summary": "user daftar",
                "tags": [
                    "users"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "role": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "berhasil daftar",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string",
                                    "example": "Berhasil daftar"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/user/login": {
            "post": {
                "summary": "user login",
                "tags": [
                    "users"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "berhasil login",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {"type": "string"},
                                        "token": {"type": "string"}
                                    },
                                    "example": {
                                        "message": "Berhasil login",
                                        "token": "eys098dufa0u325498ar834u"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}